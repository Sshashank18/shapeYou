              <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="standard">Standard trainers</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                    <div class="row">
                      
                        <% trainers.forEach(function(trainer, idx) { %>
                          <% if(trainer.subCategories.includes('CrossFit')) { %>
                            <div class="col-12">
                              <div class="container">
                              <div class="row">
                                <div class="col-lg-4">
                                 <img class="img-fluid" src="https://images.unsplash.com/photo-1527980965255-d3b416303d12?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1400&q=80" style="max-width: 50%; max-height: 50%; border-radius: 50%;" alt="">
                                </div>
                                <div class="col-lg-4">
                                  <a href="/trainer/profile/<%= trainer._id %>/<%= trainer.username %>"><h1><%= trainer.username %></h1></a>
                                </div>
                                <div class="col-lg-4">
                                  <p class="starability-result" data-rating = "3">
                                    Rated: 3 stars
                                  </p>
                                </div>
                              </div>
                              <hr style="margin-top: 0%;"> 
                            </div>
                              <!-- Rating -->
                            </div>     
                          <% } %>
                        <% }) %>
                      
                    </div>
                  </div>
                  <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                  </div>
                </div>
              </div>
            </div>

            <div class="modal fade" id="Premium" tabindex="-1" role="dialog" aria-labelledby="premium" aria-hidden="true">
              <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="premium">Premium trainers</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                    <div class="row">
                      <div class="col-lg-12">
                        <% trainers.forEach(function(trainer, idx) { %>
                          <% if(trainer.subCategories.includes('CrossFit') && trainer.categoryType == 'Premium') { %>
                            <h1>HELLO</h1>
                          <% } %>
                        <% }) %>
                          </div>
                    </div>
                  </div>
                  <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary">Save changes</button>
                  </div>
                </div>
              </div>
            </div>
</body>
</html>