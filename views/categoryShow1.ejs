      <script>
      //  $(document).on('click','.viewMore',()=>{
      //   $(this).prev('.more').css('display','block');
      //   console.log($(this));
      //  });
      const viewMore = document.querySelectorAll('.viewMore');

      for (let i = 0; i < viewMore.length; i++) {
          viewMore[i].addEventListener("click", function() {
            $(this).parent().prev().find('.more').css('display','block');
            $(this).parent().prev().find('.more').removeAttr('hidden');
            $(this).attr('hidden','');
            $(this).next().removeAttr('hidden');
          });

      }
      const viewLess = document.querySelectorAll('.viewLess');

      for (let i = 0; i < viewLess.length; i++) {
          viewLess[i].addEventListener("click", function() {
            $(this).parent().prev().find('.more').css('display','none');
            $(this).parent().prev().find('.more').attr('hidden','');
            $(this).attr('hidden','');
            $(this).prev().removeAttr('hidden');
          });
      }
    
      </script>
