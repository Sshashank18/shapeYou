<%- include('./partials/header') %>

    <h1 class="text-center" style="font-size: 50px; margin-top: 130px;;">Our categories</h1>

    <section class="text-gray-700 body-font">
  <div class="container px-5 py-24 mx-auto">
    <div class="flex flex-wrap w-full mb-20">
      <div class="lg:w-1/2 w-full mb-6 lg:mb-0">
        <h1 class="sm:text-3xl text-2xl font-medium title-font mb-2 text-gray-900">Fitness trainer</h1>
        <div class="h-1 w-20 bg-indigo-500 rounded"></div>
      </div>
      <p class="lg:w-1/2 w-full leading-relaxed text-base">Lorem ipsum, dolor sit amet consectetur adipisicing elit. Accusantium repellat iste aliquam tenetur nam, totam autem. Beatae officia ex accusamus cum illum rem veritatis voluptate, veniam sequi quos ad nisi?</p>
    </div>

    
    <div class="flex flex-wrap -m-4">
      <% categories.forEach(function(category) { %>
        <% if(category.parent == 'Fitness trainer') { %>
      <div class="xl:w-1/3 md:w-1/2 p-4 zoom">
        <div class="bg-gray-100 p-6 rounded-lg">
          <img class="h-40 rounded w-full object-cover object-center mb-6" src="https://images.unsplash.com/photo-1534258936925-c58bed479fcb?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=800&q=60" alt="content">
          <h3 class="tracking-widest text-indigo-500 text-xs font-medium title-font">SUBTITLE</h3>
          <h2 class="text-lg text-gray-900 font-medium title-font mb-4"><%= category.title %></h2>
          <p class="leading-relaxed text-base"><button class="btn btn-primary"><a href="/user/category/<%= category.parent %>">Read More</a></button></p>
        </div>
      </div>
      <% } %>
      <% }); %>


    </div>
  </div>
</section>
